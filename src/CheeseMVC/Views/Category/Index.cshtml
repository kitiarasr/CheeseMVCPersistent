
@{

//Why does adding IEnumerable to CheeseCategory thing work? Without it my model is not processed right
}
@model List<CheeseMVC.Models.CheeseCategory>


 <table class="table">
    <tr>
        <th>Name</th>
        <th>ID</th>
    </tr>


    @foreach (var category in Model)
    {
        <tr>

          <td> @category.Name </td>
          <td> @category.ID </td>


        </tr>

    }
</table>  

@if (Model.Count == 0)
{
    <p>No cheeses yet!</p>
}
<p><a asp-controller="Category" asp-action="Add">Add Category</a></p>
